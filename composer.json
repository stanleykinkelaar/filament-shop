{
    "name": "stanleykinkelaar/filament-shop",
    "description": "This is my package filament-shop",
    "keywords": [
        "stanleykinkelaar",
        "laravel",
        "filament-shop"
    ],
    "homepage": "https://github.com/stanleykinkelaar/filament-shop",
    "license": "MIT",
    "authors": [
        {
            "name": "stanley kinkelaar",
            "email": "stanley@librus.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.3",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "chiiya/filament-access-control": "1.4"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Stanleykinkelaar\\FilamentShop\\": "src",
            "Stanleykinkelaar\\FilamentShop\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stanleykinkelaar\\FilamentShop\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stanleykinkelaar\\FilamentShop\\FilamentShopServiceProvider"
            ],
            "aliases": {
                "FilamentShop": "Stanleykinkelaar\\FilamentShop\\Facades\\FilamentShop"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
